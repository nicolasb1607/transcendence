{
  "name": "source",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:generate": "pnpm exec prisma generate",
    "build:prisma": "pnpm exec prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script > ../../database/db.sql && prisma generate",
    "build": "pnpm run build:prisma && nest build",
    "build:docker": "pnpm run prisma:generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "ssl": "openssl req -x509 -newkey rsa:4096 -keyout ./secrets/key.pem -out ./secrets/cert.pem -days 365 -nodes -subj \"/C=FR/ST=Ile De France/L=Paris/O=42 School/CN=localhost\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "test": "jest",
    "test:db": "USE_DATABASE=true jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "USE_DATABASE=true jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.4.3",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/websockets": "^9.4.3",
    "@prisma/client": "4.16.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "multer": "1.4.5-lts.1",
    "notistack": "^3.0.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-oauth2": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "serve-static": "^1.15.0",
    "sharp": "^0.32.5",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.17",
	"@types/jest": "^29.5.4",
    "@types/multer": "^1.4.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/qrcode": "^1.5.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "otplib": "^12.0.1",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "qrcode": "^1.5.3",
    "run-script-webpack-plugin": "^0.2.0",
    "socket.io-client": "^4.7.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "clearMocks": true,
    "preset": "ts-jest"
  }
}
